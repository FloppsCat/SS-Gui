local function AHEPQ_fake_script() -- ChaosButton.LocalScript 
	local script = Instance.new('LocalScript', ChaosButton)

	local ChaosButton = script.Parent
	ChaosButton.MouseButton1Click:Connect(function()
		script.Parent.MouseButton1Click:Connect(function()
			local o = Instance.new("Message",workspace)
			o.Text = "Server has been destroyed by FloppsCat"
			wait(10)
			o:Destroy()
		end)
	end)
	ChaosButton.MouseButton1Click:Connect(function()
	function changeColors()
		while running do
			for _, part in ipairs(workspace:GetDescendants()) do
				if part:IsA("BasePart") then
					part.BrickColor = BrickColor.Random()
				end
			end
			wait(0.2)
		end
	end
	
	ChaosButton.MouseButton1Click:Connect(function()
		if not running then
			running = true
			spawn(changeColors)
		else
			running = false
		end
	end)
	
	end)
end
coroutine.wrap(AHEPQ_fake_script)()
