local function JQISG_fake_script() -- TextBox.LocalScript 
	local script = Instance.new('LocalScript', TextBox)

	local TextBox = script.Parent
	
	local function KillPlayer()
		local playerName = TextBox.Text
		local player = game.Players:FindFirstChild(playerName)
		if player then
			local character = player.Character
			if character then
				local humanoid = character:FindFirstChild("Humanoid")
				if humanoid then
					humanoid.Health = 0
				end
			end
		end
	end
	TextBox.FocusLost:Connect(KillPlayer)
end
coroutine.wrap(JQISG_fake_script)()
local function DXLK_fake_script() -- BringAllButton.LocalScript 
	local script = Instance.new('LocalScript', BringAllButton)

	script.Parent.MouseButton1Click:Connect(function()
		-- Получаем игрока, вызывающего скрипт
		local localPlayer = game.Players.LocalPlayer
	
		-- Функция для телепортации всех игроков к локальному игроку
		for _, player in pairs(game.Players:GetPlayers()) do
			if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				-- Проверяем, что у локального игрока есть HumanoidRootPart
				if localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart") then
					player.Character.HumanoidRootPart.Position = localPlayer.Character.HumanoidRootPart.Position
				end
			end
		end
	
	 end)
	
	
end
coroutine.wrap(DXLK_fake_script)()
