local function OTJCS_fake_script() -- PartyButton.LocalScript 
	local script = Instance.new('LocalScript', PartyButton)

	local PartyButton = script.Parent
	PartyButton.MouseButton1Click:Connect(function()
		local Lighting = game:GetService("Lighting")
		local ColorCorrection = Instance.new("ColorCorrectionEffect", Lighting)
	
		function zigzag(X) 
			return math.acos(math.cos(X*math.pi))/math.pi
		end
	
		Counter = 0
	
		Lighting.TimeOfDay = "12:00"
	
		while wait(0.1) do 
			ColorCorrection.TintColor = Color3.fromHSV(zigzag(Counter),1,1)
			Lighting.Ambient = Color3.fromHSV(zigzag(Counter),1,1)
			Counter += 0.01
		end
	end)
	
	PartyButton.MouseButton1Click:Connect(function()
			Instance.new("Sound").Parent 
				= game.Workspace  
		game.Workspace.Sound.SoundId ="rbxassetid://8867136699" 
			game.Workspace.Sound.Looped=true  
			game.Workspace.Sound:Play()
	end)
end
coroutine.wrap(OTJCS_fake_script)()
